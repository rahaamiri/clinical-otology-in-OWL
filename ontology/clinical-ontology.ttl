#################################################################
#    SWRL Rules for Safety Checks (Actual SWRL Syntax)
#################################################################

###  RULE 1: Detect Allergy-Medication Conflict via Drug Class
###  If: Patient has allergy that contraindicates drug class
###  And: Medication belongs to that drug class  
###  And: Medication is prescribed to patient
###  Then: Prescription is contraindicated
:AllergyDrugClassRule rdf:type swrl:Imp ;
    swrl:body [
        rdf:type swrl:AtomList ;
        rdf:first [
            rdf:type swrl:ClassAtom ;
            swrl:classPredicate :Patient ;
            swrl:argument1 :p
        ] ;
        rdf:rest [
            rdf:type swrl:AtomList ;
            rdf:first [
                rdf:type swrl:ClassAtom ;
                swrl:classPredicate :Allergy ;
                swrl:argument1 :a
            ] ;
            rdf:rest [
                rdf:type swrl:AtomList ;
                rdf:first [
                    rdf:type swrl:ClassAtom ;
                    swrl:classPredicate :DrugClass ;
                    swrl:argument1 :dc
                ] ;
                rdf:rest [
                    rdf:type swrl:AtomList ;
                    rdf:first [
                        rdf:type swrl:ClassAtom ;
                        swrl:classPredicate :Medication ;
                        swrl:argument1 :m
                    ] ;
                    rdf:rest [
                        rdf:type swrl:AtomList ;
                        rdf:first [
                            rdf:type swrl:ClassAtom ;
                            swrl:classPredicate :MedicationPrescription ;
                            swrl:argument1 :rx
                        ] ;
                        rdf:rest rdf:nil
                    ]
                ]
            ]
        ]
    ] ;
    swrl:head [
        rdf:type swrl:AtomList ;
        rdf:first [
            rdf:type swrl:ClassAtom ;
            swrl:classPredicate :ContraindicatedPrescription ;
            swrl:argument1 :rx
        ] ;
        rdf:rest rdf:nil
    ] .

###  RULE 2: Detect Allergy-Medication Conflict via Direct Medication
###  If: Patient has allergy that directly contraindicates medication
###  And: That medication is prescribed to patient
###  Then: Prescription is contraindicated
:AllergyMedicationRule rdf:type swrl:Imp ;
    swrl:body [
        rdf:type swrl:AtomList ;
        rdf:first [
            rdf:type swrl:ClassAtom ;
            swrl:classPredicate :Patient ;
            swrl:argument1 :p
        ] ;
        rdf:rest [
            rdf:type swrl:AtomList ;
            rdf:first [
                rdf:type swrl:ClassAtom ;
                swrl:classPredicate :Allergy ;
                swrl:argument1 :a
            ] ;
            rdf:rest [
                rdf:type swrl:AtomList ;
                rdf:first [
                    rdf:type swrl:ClassAtom ;
                    swrl:classPredicate :Medication ;
                    swrl:argument1 :m
                ] ;
                rdf:rest [
                    rdf:type swrl:AtomList ;
                    rdf:first [
                        rdf:type swrl:ClassAtom ;
                        swrl:classPredicate :MedicationPrescription ;
                        swrl:argument1 :rx
                    ] ;
                    rdf:rest rdf:nil
                ]
            ]
        ]
    ] ;
    swrl:head [
        rdf:type swrl:AtomList ;
        rdf:first [
            rdf:type swrl:ClassAtom ;
            swrl:classPredicate :ContraindicatedPrescription ;
            swrl:argument1 :rx
        ] ;
        rdf:rest rdf:nil
    ] .

###  RULE 3: Drug Interaction Warning
###  If: Two medications interact
###  And: Both are prescribed to same patient
###  Then: Mark prescriptions for review
:DrugInteractionRule rdf:type swrl:Imp ;
    swrl:body [
        rdf:type swrl:AtomList ;
        rdf:first [
            rdf:type swrl:ClassAtom ;
            swrl:classPredicate :Medication ;
            swrl:argument1 :m1
        ] ;
        rdf:rest [
            rdf:type swrl:AtomList ;
            rdf:first [
                rdf:type swrl:ClassAtom ;
                swrl:classPredicate :Medication ;
                swrl:argument1 :m2
            ] ;
            rdf:rest [
                rdf:type swrl:AtomList ;
                rdf:first [
                    rdf:type swrl:ClassAtom ;
                    swrl:classPredicate :Patient ;
                    swrl:argument1 :p
                ] ;
                rdf:rest [
                    rdf:type swrl:AtomList ;
                    rdf:first [
                        rdf:type swrl:ClassAtom ;
                        swrl:classPredicate :MedicationPrescription ;
                        swrl:argument1 :rx1
                    ] ;
                    rdf:rest [
                        rdf:type swrl:AtomList ;
                        rdf:first [
                            rdf:type swrl:ClassAtom ;
                            swrl:classPredicate :MedicationPrescription ;
                            swrl:argument1 :rx2
                        ] ;
                        rdf:rest rdf:nil
                    ]
                ]
            ]
        ]
    ] ;
    swrl:head [
        rdf:type swrl:AtomList ;
        rdf:first [
            rdf:type swrl:ClassAtom ;
            swrl:classPredicate :ContraindicatedPrescription ;
            swrl:argument1 :rx1
        ] ;
        rdf:rest [
            rdf:type swrl:AtomList ;
            rdf:first [
                rdf:type swrl:ClassAtom ;
                swrl:classPredicate :ContraindicatedPrescription ;
                swrl:argument1 :rx2
            ] ;
            rdf:rest rdf:nil
        ]
    ] .

