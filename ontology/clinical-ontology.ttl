###  Surgical Patient - Inferred from participation in surgery
:SurgicalPatient rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:intersectionOf ( :Patient
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :participatesIn ;
                                                            owl:someValuesFrom :SurgicalProcedure ] ) ] ;
                 rdfs:comment "A patient who has undergone or will undergo surgical procedures (inferred)." ;
                 rdfs:label "Surgical Patient" .

###  Patient with Hearing Loss - Inferred from diagnosis
:PatientWithHearingLoss rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Class ;
                                             owl:intersectionOf ( :Patient
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :hasDiagnosis ;
                                                                   owl:someValuesFrom :HearingLoss ] ) ] ;
                        rdfs:comment "A patient diagnosed with any type of hearing loss (inferred)." ;
                        rdfs:label "Patient with Hearing Loss" .

###  Allergic Patient - Inferred from having allergies
:AllergicPatient rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:intersectionOf ( :Patient
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :hasAllergy ;
                                                            owl:someValuesFrom :Allergy ] ) ] ;
                 rdfs:comment "A patient with documented allergies (inferred)." ;
                 rdfs:label "Allergic Patient" .

###  Patient with High-Risk Allergy - Inferred from severe allergies
:HighRiskAllergyPatient rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Class ;
                                             owl:intersectionOf ( :Patient
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :hasAllergy ;
                                                                   owl:someValuesFrom :SevereAllergy ] ) ] ;
                        rdfs:comment "A patient with severe or life-threatening allergies requiring special precautions (inferred)." ;
                        rdfs:label "High-Risk Allergy Patient" .

###  Patient Requiring Allergy Alert - Inferred from life-threatening allergies
:AllergyAlertPatient rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Class ;
                                          owl:intersectionOf ( :Patient
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :hasAllergy ;
                                                                owl:someValuesFrom :LifeThreateningAllergy ] ) ] ;
                     rdfs:comment "A patient with life-threatening allergies requiring immediate alerts (inferred)." ;
                     rdfs:label "Allergy Alert Patient" .

