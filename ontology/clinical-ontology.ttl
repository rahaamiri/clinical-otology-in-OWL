:boneConduction_1000Hz rdf:type owl:DatatypeProperty ;
                       rdfs:domain :AudiometryAssessment ;
                       rdfs:range xsd:integer ;
                       rdfs:comment "Bone conduction threshold at 1000Hz in dB HL." ;
                       rdfs:label "bone conduction 1000Hz" .

:boneConduction_2000Hz rdf:type owl:DatatypeProperty ;
                       rdfs:domain :AudiometryAssessment ;
                       rdfs:range xsd:integer ;
                       rdfs:comment "Bone conduction threshold at 2000Hz in dB HL." ;
                       rdfs:label "bone conduction 2000Hz" .

:boneConduction_4000Hz rdf:type owl:DatatypeProperty ;
                       rdfs:domain :AudiometryAssessment ;
                       rdfs:range xsd:integer ;
