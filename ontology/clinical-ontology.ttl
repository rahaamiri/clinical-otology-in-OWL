###  Other Audiometry Properties
:speechDiscriminationScore rdf:type owl:DatatypeProperty ;
                           rdfs:domain :AudiometryAssessment ;
                           rdfs:range [ rdf:type rdfs:Datatype ;
                                       owl:onDatatype xsd:decimal ;
                                       owl:withRestrictions ( [ xsd:minInclusive "0.0"^^xsd:decimal ]
                                                             [ xsd:maxInclusive "100.0"^^xsd:decimal ] ) ] ;
                           rdfs:comment "Speech discrimination score as a percentage (0-100%)." ;
                           rdfs:label "speech discrimination score" .

###  Medical Device Properties
:deviceSerialNumber rdf:type owl:DatatypeProperty , owl:FunctionalProperty ;
                    rdfs:domain :MedicalDevice ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Unique serial number for a medical device." ;
                    rdfs:label "device serial number" .

:deviceManufacturer rdf:type owl:DatatypeProperty ;
                    rdfs:domain :MedicalDevice ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Manufacturer of the medical device." ;
                    rdfs:label "device manufacturer" .

:deviceModel rdf:type owl:DatatypeProperty ;
             rdfs:domain :MedicalDevice ;
             rdfs:range xsd:string ;
             rdfs:comment "Model designation of the medical device." ;
             rdfs:label "device model" .

###  Report Properties
:reportContent rdf:type owl:DatatypeProperty ;
               rdfs:domain :MedicalReport ;
               rdfs:range xsd:string ;
               rdfs:comment "Text content of the medical report." ;
               rdfs:label "report content" .

:reportDate rdf:type owl:DatatypeProperty , owl:FunctionalProperty ;
            rdfs:domain :MedicalReport ;
            rdfs:range xsd:dateTime ;
            rdfs:comment "Date and time when the report was created." ;
            rdfs:label "report date" .

###  General Notes Property
:hasNotes rdf:type owl:DatatypeProperty ;
          rdfs:domain :ClinicalEntity ;
          rdfs:range xsd:string ;
          rdfs:comment "General notes or comments applicable to clinical entities." ;
          rdfs:label "has notes" .

#################################################################
#    Classes with Cardinality Restrictions
#################################################################

###  Root Class
:ClinicalEntity rdf:type owl:Class ;
                rdfs:comment "Root class for all clinical entities in the ontology." ;
                rdfs:label "Clinical Entity" .

###  Person Hierarchy
:Person rdf:type owl:Class ;
        rdfs:subClassOf :ClinicalEntity ;
        rdfs:comment "Base class for all persons in the healthcare system." ;
        rdfs:label "Person" .

:Patient rdf:type owl:Class ;
         rdfs:subClassOf :Person ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasMedicalRecordNumber ;
                          owl:cardinality 1 ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasDateOfBirth ;
                          owl:cardinality 1 ] ;
         owl:hasKey ( :hasMedicalRecordNumber ) ;
         rdfs:comment "A person receiving medical care. MRN must be unique." ;
         rdfs:label "Patient" .

:HealthcareProfessional rdf:type owl:Class ;
                        rdfs:subClassOf :Person ;
                        rdfs:comment "A person who provides healthcare services." ;
                        rdfs:label "Healthcare Professional" .

:Surgeon rdf:type owl:Class ;
         rdfs:subClassOf :HealthcareProfessional ;
         rdfs:comment "A healthcare professional who performs surgical procedures." ;
         rdfs:label "Surgeon" .

:Otologist rdf:type owl:Class ;
           rdfs:subClassOf :Surgeon ;
           rdfs:seeAlso <http://snomed.info/id/309343006> ;
           rdfs:comment "A surgeon specializing in ear-related conditions and procedures." ;
           rdfs:label "Otologist" .

:Audiologist rdf:type owl:Class ;
             rdfs:subClassOf :HealthcareProfessional ;
             rdfs:seeAlso <http://snomed.info/id/309418004> ;
             rdfs:comment "A healthcare professional specializing in hearing assessment and audiology." ;
             rdfs:label "Audiologist" .

###  Condition Hierarchy
:Condition rdf:type owl:Class ;
           rdfs:subClassOf :ClinicalEntity ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :diagnosisDate ;
                            owl:minCardinality 1 ] ;
           rdfs:comment "A medical condition or disease state." ;
           rdfs:label "Condition" .

:OtologicCondition rdf:type owl:Class ;
                   rdfs:subClassOf :Condition ;
                   rdfs:comment "A medical condition affecting the ear or hearing system." ;
                   rdfs:label "Otologic Condition" .

:HearingLoss rdf:type owl:Class ;
             rdfs:subClassOf :OtologicCondition ;
             rdfs:seeAlso <http://snomed.info/id/15188001> ;
             rdfs:comment "A condition characterized by reduced hearing ability." ;
             rdfs:label "Hearing Loss" .

:ConductiveHearingLoss rdf:type owl:Class ;
                       rdfs:subClassOf :HearingLoss ;
                       rdfs:comment "Hearing loss due to problems conducting sound through the outer or middle ear." ;
                       rdfs:label "Conductive Hearing Loss" .

:SensorineuralHearingLoss rdf:type owl:Class ;
