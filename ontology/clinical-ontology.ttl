@prefix : <http://www.hospital-otology.org/onto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix ruleml: <http://www.w3.org/2003/11/ruleml#> .
@base <http://www.hospital-otology.org/onto> .

<http://www.hospital-otology.org/onto> rdf:type owl:Ontology ;
                                      rdfs:comment "Comprehensive Clinical Otology Ontology - Production Ready v7.1"@en ;
                                      owl:versionInfo "7.1" ;
                                      rdfs:label "Hospital Otology Ontology" ;
                                      owl:imports <http://www.w3.org/2003/11/swrl> .

#################################################################
#    Annotation Properties
#################################################################

rdfs:seeAlso rdf:type owl:AnnotationProperty .
rdfs:label rdf:type owl:AnnotationProperty .

#################################################################
#    Object Properties
#################################################################

###  Allergy Management Properties
:hasAllergy rdf:type owl:ObjectProperty ;
            owl:inverseOf :isAllergyOf ;
            rdfs:domain :Patient ;
            rdfs:range :Allergy ;
            rdfs:comment "Links a patient to their documented allergies." ;
            rdfs:label "has allergy" .

:isAllergyOf rdf:type owl:ObjectProperty ;
             rdfs:domain :Allergy ;
             rdfs:range :Patient ;
             rdfs:comment "Indicates which patient has this allergy." ;
             rdfs:label "is allergy of" .

:contraindicatesDrugClass rdf:type owl:ObjectProperty ;
                          rdfs:domain :Allergy ;
                          rdfs:range :DrugClass ;
                          rdfs:comment "Links an allergy to drug classes that are contraindicated." ;
                          rdfs:label "contraindicates drug class" .

:contraindicatesMedication rdf:type owl:ObjectProperty ;
                           rdfs:domain :Allergy ;
                           rdfs:range :Medication ;
                           rdfs:comment "Links an allergy to specific medications that are contraindicated." ;
                           rdfs:label "contraindicates medication" .

###  Medication Properties
:belongsToDrugClass rdf:type owl:ObjectProperty ;
                    rdfs:domain :Medication ;
                    rdfs:range :DrugClass ;
                    rdfs:comment "Links a medication to its pharmacological drug class(es)." ;
                    rdfs:label "belongs to drug class" .

:interactsWith rdf:type owl:ObjectProperty , owl:SymmetricProperty ;
               rdfs:domain :Medication ;
               rdfs:range :Medication ;
               rdfs:comment "Indicates a drug-drug interaction between medications." ;
               rdfs:label "interacts with" .

###  Prescription Properties
:prescribes rdf:type owl:ObjectProperty ;
            owl:inverseOf :isPrescribedBy ;
            rdfs:domain :HealthcareProfessional ;
            rdfs:range :MedicationPrescription ;
            rdfs:comment "Links a healthcare professional to prescriptions they write." ;
            rdfs:label "prescribes" .

:isPrescribedBy rdf:type owl:ObjectProperty ;
                rdfs:domain :MedicationPrescription ;
                rdfs:range :HealthcareProfessional ;
                rdfs:comment "Indicates which healthcare professional prescribed this medication." ;
                rdfs:label "is prescribed by" .

:isPrescribedFor rdf:type owl:ObjectProperty , owl:FunctionalProperty ;
                 rdfs:domain :MedicationPrescription ;
                 rdfs:range :Patient ;
                 rdfs:comment "Links a prescription to the patient it is for. Each prescription is for exactly one patient." ;
                 rdfs:label "is prescribed for" .

:hasPrescribedMedication rdf:type owl:ObjectProperty , owl:FunctionalProperty ;
                         rdfs:domain :MedicationPrescription ;
                         rdfs:range :Medication ;
                         rdfs:comment "Links a prescription to the specific medication. Each prescription has exactly one medication." ;
                         rdfs:label "has prescribed medication" .

###  Condition and Diagnosis Properties
:affectsStructure rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isAffectedBy ;
                  rdfs:domain :Condition ;
                  rdfs:range :AnatomicalStructure ;
                  rdfs:comment "Links a condition to the anatomical structure(s) it affects." ;
                  rdfs:label "affects structure" .

:isAffectedBy rdf:type owl:ObjectProperty ;
              rdfs:domain :AnatomicalStructure ;
              rdfs:range :Condition ;
              rdfs:comment "Indicates which condition(s) affect this anatomical structure." ;
              rdfs:label "is affected by" .

:hasDiagnosis rdf:type owl:ObjectProperty ;
              owl:inverseOf :isDiagnosisOf ;
              rdfs:domain :Patient ;
              rdfs:range :Condition ;
              rdfs:comment "Links a patient to their diagnosed conditions." ;
              rdfs:label "has diagnosis" .

:isDiagnosisOf rdf:type owl:ObjectProperty ;
               rdfs:domain :Condition ;
               rdfs:range :Patient ;
               rdfs:comment "Indicates which patient has this diagnosis." ;
               rdfs:label "is diagnosis of" .

###  Clinical Process Properties
:isPerformedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf :performs ;
               rdfs:domain :ClinicalProcess ;
               rdfs:range :HealthcareProfessional ;
               rdfs:comment "Links a clinical process to the healthcare professional(s) who perform it." ;
               rdfs:label "is performed by" .

:performs rdf:type owl:ObjectProperty ;
          rdfs:domain :HealthcareProfessional ;
          rdfs:range :ClinicalProcess ;
          rdfs:comment "Indicates which clinical processes a healthcare professional performs." ;
          rdfs:label "performs" .

:hasPrimarySurgeon rdf:type owl:ObjectProperty , owl:FunctionalProperty ;
                   rdfs:subPropertyOf :isPerformedBy ;
                   owl:inverseOf :isPrimarySurgeonFor ;
                   rdfs:domain :SurgicalProcedure ;
                   rdfs:range :Surgeon ;
                   rdfs:comment "Identifies the primary surgeon for a surgical procedure. Each surgery has exactly one primary surgeon." ;
                   rdfs:label "has primary surgeon" .

:isPrimarySurgeonFor rdf:type owl:ObjectProperty ;
                     rdfs:domain :Surgeon ;
                     rdfs:range :SurgicalProcedure ;
                     rdfs:comment "Indicates which surgical procedures this surgeon is the primary surgeon for." ;
                     rdfs:label "is primary surgeon for" .

:hasAssistingSurgeon rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :isPerformedBy ;
                     owl:inverseOf :assistsInSurgery ;
                     rdfs:domain :SurgicalProcedure ;
                     rdfs:range :Surgeon ;
                     rdfs:comment "Identifies assisting surgeons in a surgical procedure." ;
                     rdfs:label "has assisting surgeon" .

:assistsInSurgery rdf:type owl:ObjectProperty ;
                  rdfs:domain :Surgeon ;
                  rdfs:range :SurgicalProcedure ;
                  rdfs:comment "Indicates which surgeries this surgeon assists in." ;
                  rdfs:label "assists in surgery" .

:participatesIn rdf:type owl:ObjectProperty ;
                owl:inverseOf :hasParticipant ;
                rdfs:domain :Patient ;
                rdfs:range :ClinicalProcess ;
                rdfs:comment "Links a patient to clinical processes they participate in." ;
                rdfs:label "participates in" .

:hasParticipant rdf:type owl:ObjectProperty ;
                rdfs:domain :ClinicalProcess ;
                rdfs:range :Patient ;
                rdfs:comment "Links a clinical process to the patient participant(s)." ;
                rdfs:label "has participant" .

###  Anatomical Properties
:isPartOf rdf:type owl:ObjectProperty , owl:TransitiveProperty ;
          owl:inverseOf :hasPart ;
          rdfs:domain :AnatomicalStructure ;
          rdfs:range :AnatomicalStructure ;
          rdfs:comment "Represents the anatomical part-whole relationship. Transitive property." ;
          rdfs:label "is part of" .

:hasPart rdf:type owl:ObjectProperty , owl:TransitiveProperty ;
         rdfs:domain :AnatomicalStructure ;
         rdfs:range :AnatomicalStructure ;
         rdfs:comment "Inverse of isPartOf - indicates parts of an anatomical structure." ;
         rdfs:label "has part" .

###  Treatment Properties
:isIndicatedFor rdf:type owl:ObjectProperty ;
                owl:inverseOf :treatedBy ;
                rdfs:domain :TreatmentProcedure ;
                rdfs:range :Condition ;
                rdfs:comment "Links a treatment to the condition(s) it is designed to address." ;
                rdfs:label "is indicated for" .

:treatedBy rdf:type owl:ObjectProperty ;
           rdfs:domain :Condition ;
           rdfs:range :TreatmentProcedure ;
           rdfs:comment "Links a condition to the treatment procedure(s) used to address it." ;
           rdfs:label "treated by" .

###  Device Properties
:usesDevice rdf:type owl:ObjectProperty ;
            owl:inverseOf :isUsedIn ;
            rdfs:domain :ClinicalProcess ;
            rdfs:range :MedicalDevice ;
            rdfs:comment "Links a clinical process to medical devices used." ;
            rdfs:label "uses device" .

:isUsedIn rdf:type owl:ObjectProperty ;
          rdfs:domain :MedicalDevice ;
          rdfs:range :ClinicalProcess ;
          rdfs:comment "Indicates which clinical processes use this medical device." ;
          rdfs:label "is used in" .

:requiresDevice rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :usesDevice ;
                rdfs:domain :SurgicalProcedure ;
                rdfs:range :MedicalDevice ;
                rdfs:comment "Indicates mandatory medical devices for a surgical procedure." ;
                rdfs:label "requires device" .

###  Laterality Properties
:hasLaterality rdf:type owl:ObjectProperty , owl:FunctionalProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :AnatomicalStructure :Condition :DiagnosticProcedure ) ] ;
               rdfs:range :Laterality ;
               rdfs:comment "Indicates the side affected (left, right, or bilateral). Functional property ensures only one laterality per entity." ;
               rdfs:label "has laterality" .

###  Outcome and Follow-up Properties
:hasOutcome rdf:type owl:ObjectProperty ;
            owl:inverseOf :isOutcomeOf ;
            rdfs:domain :ClinicalProcess ;
            rdfs:range :ProcedureOutcome ;
            rdfs:comment "Links a clinical process to its outcome." ;
            rdfs:label "has outcome" .

:isOutcomeOf rdf:type owl:ObjectProperty ;
             rdfs:domain :ProcedureOutcome ;
             rdfs:range :ClinicalProcess ;
             rdfs:comment "Indicates which clinical process this outcome belongs to." ;
             rdfs:label "is outcome of" .

:hasComplication rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isComplicationOf ;
                 rdfs:domain :ClinicalProcess ;
                 rdfs:range :Complication ;
                 rdfs:comment "Links a clinical process to any complications that occurred." ;
                 rdfs:label "has complication" .

:isComplicationOf rdf:type owl:ObjectProperty ;
                  rdfs:domain :Complication ;
                  rdfs:range :ClinicalProcess ;
                  rdfs:comment "Indicates which clinical process this complication occurred during." ;
                  rdfs:label "is complication of" .

:hasFollowUp rdf:type owl:ObjectProperty ;
             owl:inverseOf :isFollowUpFor ;
             rdfs:domain :ClinicalProcess ;
             rdfs:range :FollowUpAppointment ;
             rdfs:comment "Links a clinical process to scheduled follow-up appointments." ;
             rdfs:label "has follow-up" .

:isFollowUpFor rdf:type owl:ObjectProperty ;
               rdfs:domain :FollowUpAppointment ;
               rdfs:range :ClinicalProcess ;
               rdfs:comment "Indicates which clinical process this is a follow-up for." ;
               rdfs:label "is follow-up for" .

###  Report Properties
:hasReport rdf:type owl:ObjectProperty ;
           owl:inverseOf :isReportFor ;
           rdfs:domain :ClinicalProcess ;
           rdfs:range :MedicalReport ;
           rdfs:comment "Links a clinical process to associated medical reports." ;
           rdfs:label "has report" .

:isReportFor rdf:type owl:ObjectProperty ;
             rdfs:domain :MedicalReport ;
             rdfs:range :ClinicalProcess ;
             rdfs:comment "Indicates which clinical process this report documents." ;
             rdfs:label "is report for" .

#################################################################
#    Data Properties
#################################################################

###  Patient Demographics
:hasMedicalRecordNumber rdf:type owl:DatatypeProperty , owl:FunctionalProperty ;
                        rdfs:domain :Patient ;
                        rdfs:range xsd:string ;
                        rdfs:comment "Unique medical record number. Enforced by owl:hasKey on Patient class." ;
                        rdfs:label "has medical record number" .

:hasDateOfBirth rdf:type owl:DatatypeProperty , owl:FunctionalProperty ;
                rdfs:domain :Patient ;
                rdfs:range xsd:date ;
                rdfs:comment "Patient's date of birth. Each patient has exactly one birth date." ;
                rdfs:label "has date of birth" .

:hasGender rdf:type owl:DatatypeProperty , owl:FunctionalProperty ;
           rdfs:domain :Patient ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                       owl:oneOf ( "Male"^^xsd:string "Female"^^xsd:string "Other"^^xsd:string "Unknown"^^xsd:string ) ] ;
           rdfs:comment "Patient's gender." ;
           rdfs:label "has gender" .

:hasWeightKg rdf:type owl:DatatypeProperty ;
             rdfs:domain :Patient ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:onDatatype xsd:decimal ;
                         owl:withRestrictions ( [ xsd:minInclusive "0.5"^^xsd:decimal ]
                                                [ xsd:maxInclusive "300.0"^^xsd:decimal ] ) ] ;
             rdfs:comment "Patient's weight in kilograms." ;
             rdfs:label "has weight (kg)" .

:hasHeightCm rdf:type owl:DatatypeProperty ;
             rdfs:domain :Patient ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:onDatatype xsd:decimal ;
                         owl:withRestrictions ( [ xsd:minInclusive "30.0"^^xsd:decimal ]
                                                [ xsd:maxInclusive "250.0"^^xsd:decimal ] ) ] ;
             rdfs:comment "Patient's height in centimeters." ;
             rdfs:label "has height (cm)" .

:hasContactPhone rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Patient ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Patient's contact phone number." ;
                 rdfs:label "has contact phone" .

:hasContactEmail rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Patient ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Patient's contact email address." ;
                 rdfs:label "has contact email" .

###  Allergy Properties
:hasAllergySeverity rdf:type owl:DatatypeProperty , owl:FunctionalProperty ;
                    rdfs:domain :Allergy ;
                    rdfs:range [ rdf:type rdfs:Datatype ;
                                owl:oneOf ( "Mild"^^xsd:string "Moderate"^^xsd:string "Severe"^^xsd:string "Life-threatening"^^xsd:string ) ] ;
                    rdfs:comment "Severity level of the allergy (enumerated values)." ;
                    rdfs:label "has allergy severity" .

:allergyReaction rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Allergy ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Description of the allergic reaction." ;
                 rdfs:label "allergy reaction" .

:allergyFirstObservedDate rdf:type owl:DatatypeProperty ;
                          rdfs:domain :Allergy ;
                          rdfs:range xsd:date ;
                          rdfs:comment "Date when allergy was first observed." ;
                          rdfs:label "allergy first observed date" .

###  Prescription Properties
:prescriptionDate rdf:type owl:DatatypeProperty , owl:FunctionalProperty ;
                  rdfs:domain :MedicationPrescription ;
                  rdfs:range xsd:dateTime ;
                  rdfs:comment "Date and time when the medication was prescribed." ;
                  rdfs:label "prescription date" .

:medicationDosage rdf:type owl:DatatypeProperty ;
                  rdfs:domain :MedicationPrescription ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Dosage amount and unit (e.g., '500mg')." ;
                  rdfs:label "medication dosage" .

:medicationFrequency rdf:type owl:DatatypeProperty ;
                     rdfs:domain :MedicationPrescription ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Frequency of medication administration (e.g., 'Three times daily')." ;
                     rdfs:label "medication frequency" .

:medicationDurationDays rdf:type owl:DatatypeProperty ;
                        rdfs:domain :MedicationPrescription ;
                        rdfs:range xsd:integer ;
                        rdfs:comment "Duration of medication course in days." ;
                        rdfs:label "medication duration (days)" .

:routeOfAdministration rdf:type owl:DatatypeProperty ;
                       rdfs:domain :MedicationPrescription ;
                       rdfs:range [ rdf:type rdfs:Datatype ;
                                   owl:oneOf ( "Oral"^^xsd:string "Intravenous"^^xsd:string "Intramuscular"^^xsd:string 
                                              "Subcutaneous"^^xsd:string "Topical"^^xsd:string "Otic"^^xsd:string "Other"^^xsd:string ) ] ;
                       rdfs:comment "Route of medication administration." ;
                       rdfs:label "route of administration" .

###  Medication Properties
:activeIngredient rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Medication ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Primary active pharmaceutical ingredient." ;
                  rdfs:label "active ingredient" .

:brandName rdf:type owl:DatatypeProperty ;
           rdfs:domain :Medication ;
           rdfs:range xsd:string ;
           rdfs:comment "Brand/trade name of the medication." ;
           rdfs:label "brand name" .

:genericName rdf:type owl:DatatypeProperty ;
             rdfs:domain :Medication ;
             rdfs:range xsd:string ;
             rdfs:comment "Generic/scientific name of the medication." ;
             rdfs:label "generic name" .

###  Temporal Properties for Conditions
:diagnosisDate rdf:type owl:DatatypeProperty ;
               rdfs:domain :Condition ;
               rdfs:range xsd:date ;
               rdfs:comment "Date when the condition was diagnosed." ;
               rdfs:label "diagnosis date" .

:onsetDate rdf:type owl:DatatypeProperty ;
           rdfs:domain :Condition ;
           rdfs:range xsd:date ;
           rdfs:comment "Date when the condition symptoms first appeared." ;
           rdfs:label "onset date" .

:resolutionDate rdf:type owl:DatatypeProperty ;
                rdfs:domain :Condition ;
                rdfs:range xsd:date ;
                rdfs:comment "Date when the condition was resolved (if applicable)." ;
                rdfs:label "resolution date" .

###  Clinical Process Temporal Properties
:procedureStartDate rdf:type owl:DatatypeProperty , owl:FunctionalProperty ;
                    rdfs:domain :ClinicalProcess ;
                    rdfs:range xsd:dateTime ;
                    rdfs:comment "Start date and time of the clinical procedure." ;
                    rdfs:label "procedure start date" .

:procedureEndDate rdf:type owl:DatatypeProperty , owl:FunctionalProperty ;
                  rdfs:domain :ClinicalProcess ;
                  rdfs:range xsd:dateTime ;
                  rdfs:comment "End date and time of the clinical procedure." ;
                  rdfs:label "procedure end date" .

