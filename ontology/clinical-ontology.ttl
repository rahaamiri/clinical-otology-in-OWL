###  Main entity type disjointness
[] rdf:type owl:AllDisjointClasses ;
   owl:members ( :Person :ClinicalProcess :Condition :AnatomicalStructure 
                 :Medication :MedicationPrescription :DrugClass :Allergy 
                 :MedicalDevice :Laterality :ProcedureOutcome :Complication 
                 :MedicalReport ) .

###  Person hierarchy disjointness
[] rdf:type owl:AllDisjointClasses ;
   owl:members ( :Patient :HealthcareProfessional ) .

###  Healthcare professional specialties disjointness
[] rdf:type owl:AllDisjointClasses ;
   owl:members ( :Surgeon :Audiologist ) .

###  Clinical process type disjointness
[] rdf:type owl:AllDisjointClasses ;
   owl:members ( :DiagnosticProcedure :TreatmentProcedure :Consultation ) .

###  Hearing loss type disjointness
[] rdf:type owl:AllDisjointClasses ;
   owl:members ( :ConductiveHearingLoss :SensorineuralHearingLoss :MixedHearingLoss ) .

###  Ear structure disjointness
[] rdf:type owl:AllDisjointClasses ;
   owl:members ( :ExternalEar :MiddleEar :InnerEar ) .

###  Ossicle disjointness
[] rdf:type owl:AllDisjointClasses ;
   owl:members ( :Malleus :Incus :Stapes ) .

###  Outcome disjointness
[] rdf:type owl:AllDisjointClasses ;
   owl:members ( :SuccessfulOutcome :UnsuccessfulOutcome :PartiallySuccessfulOutcome ) .

###  Allergy severity subclass disjointness
[] rdf:type owl:AllDisjointClasses ;
   owl:members ( :MildAllergy :SevereAllergy :LifeThreateningAllergy ) .

